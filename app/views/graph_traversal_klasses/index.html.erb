<p id="notice"><%= notice %></p>

<h1>Graph Traversal Klasses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Json data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graph_traversal_klasses.each do |graph_traversal_klass| %>
      <tr>
        <td><%= graph_traversal_klass.name %></td>
        <td><%= graph_traversal_klass.json_data %></td>
        <td><%= link_to 'Show', graph_traversal_klass %></td>
        <td><%= link_to 'Edit', edit_graph_traversal_klass_path(graph_traversal_klass) %></td>
        <td><%= link_to 'Destroy', graph_traversal_klass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Graph Traversal Klass', new_graph_traversal_klass_path %>
